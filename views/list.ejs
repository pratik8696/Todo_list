<%- include("header") -%>

  <div class="box" id="heading">
    <h1><%= todays_date %></h1>
  </div>
  <div class="box">
    <%  for(var i=0;i<newitem.length;i++){  %>
    <div class="item">
      <input type="checkbox" />
      <p> <%= newitem[i] %> </p>
    </div>
    <%  }  %>
    <form action="/" method="post">
      <input type="text" name="newitem_here" placeholder="New Item Here" autocomplete="off">
      <button type="submit" name="list" value="<%= todays_date %>">+</button>
    </form>
  </div>

<%- include("footer") -%>
